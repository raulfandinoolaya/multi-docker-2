{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "calcas0731/multi-client",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "server",
      "image": "calcas0731/multi-server",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "worker",
      "image": "calcas0731/multi-worker",
      "hostname": "worker",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "calcas0731/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80,
          
        }
      ],
      "links": [
        "client",
        "server"
      ]
    }
  ]
}